#!/bin/bash

# Get the values of the environment variables WATER and STIR
WATER="$WATER"
STIR="$STIR"

# Reverse the strings
WATER_REVERSED=$(echo "$WATER" | rev)
STIR_REVERSED=$(echo "$STIR" | rev)

# Concatenate the reversed strings
RESULT="${WATER_REVERSED}${STIR_REVERSED}"

# Print the result
echo "$RESULT"
